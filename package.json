    "@types/react": "18.2.0",
    "@types/react-collapse": "^5.0.4",
    "@types/react-dom": "18.2.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "dotenv-cli": "^7.3.0",
    "encoding": "^0.1.13",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "postgres": "^3.4.3",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1"
