{
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "prisma generate && next build",
    "start": "next start",
    "migrate-dev": "dotenv -e .env.development.local -- prisma migrate dev && bun run lib/seedTriggers.ts",
    "migrate-prod": "dotenv -e .env.production.local -- prisma migrate dev && bun run lib/seedTriggers.ts",
    "seed": "NODE_ENV=development && bun run data/seed.ts",
    "seed-prod": "NODE_ENV=production && bun run data/seedProd.ts",
    "lint": "next lint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nextui-org/react": "^2.2.9",
    "@prisma/client": "5.7.1",
    "@stripe/react-stripe-js": "^2.4.0",
    "@stripe/stripe-js": "^2.2.2",
    "@supabase/ssr": "latest",
    "@supabase/supabase-js": "latest",
    "@types/fold-to-ascii": "^5.0.2",
    "autoprefixer": "10.4.15",
    "cssnano": "^6.0.2",
    "fold-to-ascii": "^5.0.1",
    "framer-motion": "^10.17.0",
    "geist": "^1.0.0",
    "next": "latest",
    "next-themes": "^0.2.1",
    "postcss": "^8.4.32",
    "react": "18.2.0",
    "react-collapse": "^5.1.1",
    "react-dom": "18.2.0",
    "react-icons": "^4.12.0",
    "sharp": "^0.33.1",
    "sonner": "^1.3.1",
    "stripe": "^14.11.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/bun": "^1.0.0",
    "@types/node": "20.3.1",
    "@types/react": "18.2.0",
    "@types/react-collapse": "^5.0.4",
    "@types/react-dom": "18.2.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "dotenv-cli": "^7.3.0",
    "encoding": "^0.1.13",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "postgres": "^3.4.3",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint ./ -c .eslintrc.json --quiet --format visualstudio",
      "prettier ./ --write --ignore-unknown"
    ]
  }
}
