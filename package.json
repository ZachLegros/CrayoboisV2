{
  "private": true,
  "scripts": {
    "dev": "bun clean && next dev --turbo",
    "build": "bun generate && next build",
    "start": "next start",
    "generate": "prisma generate",
    "clean": "bun run clean.ts",
    "lint": "biome check --no-errors-on-unmatched --files-ignore-unknown=true --apply .",
    "migrate": "dotenv -e .env.development.local -- prisma migrate dev && bun seed-triggers",
