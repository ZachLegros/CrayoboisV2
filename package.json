    "seed-triggers": "bun --env-file=.env.development.local lib/seedTriggers.ts",
    "migrate:prod": "dotenv -e .env.production.local -- prisma migrate deploy && bun seed-triggers-prod",
    "seed:prod": "bun --env-file=.env.production.local data/seed.ts",
    "seed-triggers:prod": "bun --env-file=.env.production.local lib/seedTriggers.ts",
    "studio": "dotenv -e .env.development.local -- prisma studio",
    "prepare": "husky install"
  },
